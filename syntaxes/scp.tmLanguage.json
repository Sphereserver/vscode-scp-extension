{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "scp",
	"patterns": [
		{
			"include": "#comment"
		},
		{
			"include": "#headers"
		},
		{
			"include": "#string"
		},
		{
			"include": "#ctrl-flow-statement"
		},
		{
			"include": "#conditional-expression"
		},
		{
			"include": "#parser-modifier"
		},
		{
			"include": "#object-builtin"
		},
		{
			"include": "#storage-builtin"
		},
		{
			"include": "#variable-reserved"
		},
		{
			"include": "#function-intrinsic"
		},
		{
			"include": "#trigger-builtin"
		},
		{
			"include": "#property-builtin"
		},
		{
			"include": "#function-builtin"
		},
		{
			"include": "#operator"
		},
		{
			"include": "#numeric-literal"
		}
	],
	"repository": {
		"string": {
			"name": "string.quoted.double.scp",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.scp",
					"match": "\\\\.*"
				}
			]
		},
		"comment": {
			"name": "comment.scp",
			"patterns": [
				{
					"include": "#comment-block"
				},
				{
					"include": "#comment-multiline"
				},
				{
					"include": "#comment-line"
				}
			]
		},
		"comment-block": {
			"begin": "(?i)^\\s*\\[COMMENT.*\\]",
			"end": "\\^\\s*[.*\\]",
			"name": "comment.block.documentation.scp"
		},
		"comment-multiline": {
			"begin": "/\\*",
			"end": "\\*/",
			"name": "comment.block.multiline.scp"
		},
		"comment-line": {
			"match": "(//).*$\\n?",
			"name": "comment.line.scp"
		},
		"headers": {
			"name": "entity.name.type.header.scp",
			"begin": "(^\\s*\\[)",
			"end": "(\\])",
			"patterns": [
				{
					"name": "entity.name.type.declaration.scp",
					"match": "(?i)(areadef|chardef|defnames|dialog|eof|events|function|itemdef|menu|multidef|regionresource|regiontype|roomdef|speech|template|typedef)"
				}
			]
		},
		"operator": {
			"name": "keyword.operator.scp",
			"match": "(~|<|>|<=|>=|\\|&|&&|<<|>>|!|!=|=|==|\\?|\\^|@|%|\/|\\*|-|\\+)"
		},
		"numeric-literal": {
      "match": "\\s*(?<=[^$])((0(x|X)[0-9a-fA-F]+)|([0-9]+(\\.[0-9]+)?))\\b",
      "name": "constant.numeric.scp"
    },
		"conditional-expression": {
			"name": "keyword.control.conditional.scp",
			"match": "(?i)\\b(if|else|elif|elseif|endif)\\b"
		},
		"ctrl-flow-statement": {
			"name": "keyword.control.flow.scp",
			"match": "(?i)\\b(begin|break|continue|doswitch|end|enddo|endfor|endwhile|for|forchars|forcharlayer|forcharmemorytype|forclients|forcont|forcontid|forconttype|forinstances|foritems|forobjs|forplayers|switch|while)\\b"
		},
		"variable-reserved": {
			"name": "variable.name.scp",
			"match": "(?i)\\b(_for|_while)\\b"
		},
		"parser-modifier": {
			"name": "keyword.other.scp",
			"match": "(?i)\\b(call|fullinterp|on|try|tryp|trysrc|trysrv)\\b"
		},
		"function-intrinsic": {
			"name": "entity.name.function.intrinsic.scp",
			"match": "(?i)\\b(a|asc|b|d|eval|explode|f|floatval|hval|isempty|isnum|isnumber|isvalid|newitem|newnpc|qval|r|rand|return|s|strarg|strcmp|strcmpi|streat|streverse|strlen|strmatch|strmid|strpos|strregex|strrev|strsub|strtoupper|strtrim|w)\\b"
		},
		"storage-builtin": {
			"name": "storage.type.scp",
			"match": "(?i)\\b(d?def0?|defmsg|d?tag0?|d?ctag0?|d?var0?|d?local|float|ARGO|ARGS|ARGV|ARG|ARGCHK|ARGCHKID|ARGTXT|d?ARGN[1-3])\\b"
		},
		"object-builtin": {
			"name": "entity.name.class.scp",
			"match": "(?i)\\b(account|act|argo|chardef|client|comp|cont|db|file|findcont|findid|findlayer|findtype|finduid|gmpage|guild|itemdef|lastnew|link|map|member|multi|new|obj|owner|party|ref[1-255]|region|sector|sectors|serv|skill|spell|src|targ|topobj|typedef|town)\\b"
		},
